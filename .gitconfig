[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f

[user]
	name = Gyubong Lee
	email = jopemachine@naver.com

[core]
	precomposeunicode = true
	quotepath = false
	excludesfile = /Users/igyubong/.gitignore

[diff]
	noprefix = false

[advice]
	detachedHead = true

[init]
	defaultBranch = master

[pull]
	ff = only

[alias]
	cf = forgit checkout_file

[core]
    pager = delta

[interactive]
    diffFilter = delta --color-only

[delta]
    navigate = true  # use n and N to move between diff sections

[merge]
    conflictstyle = diff3

[diff]
    colorMoved = default
